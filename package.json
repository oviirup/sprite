{
  "name": "@oviirup/sprite",
  "version": "0.0.9",
  "displayName": "sprite",
  "description": "Create svg sprite from svg files",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/sprite"
  },
  "author": "Avirup Ghosh (https://github.com/oviirup)",
  "license": "MIT",
  "bin": "./dist/bin.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./bin": {
      "require": "./dist/bin.js",
      "import": "./dist/bin.mjs"
    }
  },
  "keywords": [
    "svg-sprite",
    "cli"
  ],
  "scripts": {
    "build": "bunchee -m",
    "check:format": "prettier --check -u .",
    "check:types": "tsc --noEmit",
    "dev": "bunchee -w",
    "format": "prettier -w -u --cache .",
    "prepack": "husky",
    "start": "tsx ./src/bin.ts"
  },
  "dependencies": {
    "chokidar": "^3",
    "commander": "^12",
    "cosmiconfig": "^9",
    "node-html-parser": "^6",
    "picocolors": "^1",
    "svgo": "^3"
  },
  "devDependencies": {
    "@changesets/cli": "^2",
    "@types/node": "^20",
    "bunchee": "^5",
    "husky": "^9",
    "lint-staged": "^15",
    "prettier": "^3",
    "prettier-plugin-imports": "^4.2",
    "prettier-plugin-jsdoc": "^1.3",
    "prettier-plugin-pkg": "^0.18",
    "tsx": "^4",
    "typescript": "^5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "lint-staged": {
    "*": "prettier -w -u"
  }
}
