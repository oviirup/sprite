{
  "name": "@oviirup/sprite",
  "version": "0.1.2",
  "displayName": "sprite",
  "description": "Create svg sprite sheet from SVG files, with ease",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/sprite"
  },
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup/"
  },
  "license": "MIT",
  "packageManager": "bun@1.1.34",
  "bin": {
    "sprite": "./dist/bin.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./bin": {
      "import": "./dist/bin.mjs",
      "default": "./dist/bin.js"
    },
    "./lib/build": {
      "types": "./dist/lib/build.d.ts",
      "import": "./dist/lib/build.mjs",
      "default": "./dist/lib/build.js"
    },
    "./lib/init": {
      "types": "./dist/lib/init.d.ts",
      "import": "./dist/lib/init.mjs",
      "default": "./dist/lib/init.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "svg",
    "svg-sprite",
    "cli"
  ],
  "scripts": {
    "build": "bunchee -m",
    "check:format": "prettier --check -u .",
    "check:types": "tsc --noEmit",
    "clean": "npx rimraf node_modules dist",
    "dev": "bunchee -w",
    "format": "prettier -w -u --cache ."
  },
  "dependencies": {
    "chokidar": "~4.0.3",
    "commander": "~13.0.0",
    "fast-glob": "~3.3.2",
    "node-html-parser": "~7.0.1",
    "picocolors": "~1.1.1",
    "yaml": "~2.7.0",
    "zod": "~3.24.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "~0.5.0",
    "@changesets/cli": "~2.27.11",
    "@types/node": "~20.17.11",
    "bunchee": "~6.2.0",
    "prettier": "~3.4.2",
    "prettier-plugin-imports": "~4.2.12",
    "prettier-plugin-jsdoc": "~1.3.0",
    "prettier-plugin-pkg": "~0.18.1",
    "typescript": "~5.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
