{
  "name": "@oviirup/sprite",
  "version": "0.0.0",
  "type": "module",
  "displayName": "sprite",
  "description": "Create svg sprite from svg files",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/sprite"
  },
  "author": "Avirup Ghosh (https://github.com/oviirup)",
  "license": "MIT",
  "bin": "./dist/bin.cjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./bin": {
      "require": "./dist/bin.cjs"
    }
  },
  "keywords": [
    "svg-sprite",
    "cli"
  ],
  "scripts": {
    "build": "bunchee -m",
    "format": "prettier -w -u --cache .",
    "prepack": "husky",
    "start": "tsx ./src/bin.ts"
  },
  "dependencies": {
    "commander": "^12",
    "cosmiconfig": "^9",
    "node-html-parser": "^6",
    "picocolors": "^1",
    "svgo": "^3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "bunchee": "^5.2.1",
    "husky": "^9",
    "lint-staged": "^15",
    "prettier": "^3",
    "prettier-plugin-imports": "^4.2",
    "prettier-plugin-jsdoc": "^1.3",
    "prettier-plugin-pkg": "^0.18",
    "tsx": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*": "prettier -w -u"
  }
}
